	-I$(top_srcdir) \
	-I$(top_srcdir)/include \
	-I$(top_srcdir)/VEX/pub \
	-DVGA_amd64=1 \
	-DVGO_freebsd=1 \
	-DVGP_amd64_freebsd=1 \
	-DVGPV_amd64_freebsd_vanilla=1

AM_CPPFLAGS_X86_FREEBSD = \
	-I$(top_srcdir) \
