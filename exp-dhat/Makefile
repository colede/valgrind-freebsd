exp_dhat_amd64_freebsd-dh_main.o: dh_main.c
	$(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(exp_dhat_amd64_freebsd_CPPFLAGS) $(CPPFLAGS) $(exp_dhat_amd64_freebsd_CFLAGS) $(CFLAGS) -MT exp_dhat_amd64_freebsd-dh_main.o -MD -MP -MF $(DEPDIR)/exp_dhat_amd64_freebsd-dh_main.Tpo -c -o exp_dhat_amd64_freebsd-dh_main.o `test -f 'dh_main.c' || echo '$(srcdir)/'`dh_main.c
	$(am__mv) $(DEPDIR)/exp_dhat_amd64_freebsd-dh_main.Tpo $(DEPDIR)/exp_dhat_amd64_freebsd-dh_main.Po
#	source='dh_main.c' object='exp_dhat_amd64_freebsd-dh_main.o' libtool=no \
#	DEPDIR=$(DEPDIR) $(CCDEPMODE) $(depcomp) \
#	$(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(exp_dhat_amd64_freebsd_CPPFLAGS) $(CPPFLAGS) $(exp_dhat_amd64_freebsd_CFLAGS) $(CFLAGS) -c -o exp_dhat_amd64_freebsd-dh_main.o `test -f 'dh_main.c' || echo '$(srcdir)/'`dh_main.c

